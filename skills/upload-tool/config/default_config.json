{
    "compression_format": "zip",
    "cleanup_temp": true,
    "verify_upload": true,
    "retry_count": 3,
    "retry_delay": 5,
    "exclude_patterns": [
        "__pycache__",
        ".git",
        "*.pyc",
        ".DS_Store",
        "node_modules",
        "*.log",
        "temp",
        "*.tmp"
    ],
    "preserve_permissions": true,
    "preserve_timestamps": true
}
